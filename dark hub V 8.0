-- Button click handlers with error handling
Button1.MouseButton1Click:Connect(function()
    local success, result = pcall(function()
        -- Check if getgenv exists
        if type(_G.getgenv) == "function" then
            pcall(function() _G.DisableWarning = false end)
        end
        
        local success, content = pcall(function()
            return game:HttpGet("https://raw.githubusercontent.com/skibiditoiletfan2007/KadeHubRepository/main/Latest.lua")
        end)
        
        if success and content then
            loadstring(content)()
        else
            warn("Failed to load KadeHub:", content)
        end
    end)
    
    if not success then
        warn("Error executing KadeHub:", result)
    end
end)

